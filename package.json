{
  "name": "nestjs-dynamoose",
  "version": "0.5.0",
  "description": "Nest - modern, fast, powerful node.js web framework (@dynamoose)",
  "author": "Hardys",
  "repository": "https://github.com/hardyscc/nestjs-dynamoose.git",
  "license": "MIT",
  "scripts": {
    "lint": "eslint \"lib/**/*.ts\" --max-warnings 0",
    "format": "prettier \"lib/**/*.ts\" --write",
    "build": "rm -rf dist && tsc -p tsconfig.json",
    "precommit": "lint-staged",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "prepublish:next": "npm run build",
    "publish:next": "npm publish --access public --tag next",
    "prerelease": "npm run build",
    "release": "release-it"
  },
  "devDependencies": {
    "@commitlint/cli": "17.1.2",
    "@commitlint/config-angular": "17.1.0",
    "@nestjs/common": "9.1.2",
    "@nestjs/core": "9.1.2",
    "@types/node": "16.11.62",
    "@typescript-eslint/eslint-plugin": "5.38.0",
    "@typescript-eslint/parser": "5.38.0",
    "dynamoose": "3.1.0",
    "eslint": "8.24.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "reflect-metadata": "0.1.13",
    "release-it": "15.4.2",
    "rxjs": "7.5.7",
    "rxjs-compat": "6.6.7",
    "ts-node": "10.9.1",
    "typescript": "4.8.4"
  },
  "peerDependencies": {
    "@aws-sdk/client-dynamodb": "^3.0.0",
    "@nestjs/common": "^6.0.0 || ^7.0.0 || ^8.0.0 || ^9.0.0",
    "@nestjs/core": "^6.0.0 || ^7.0.0 || ^8.0.0 || ^9.0.0",
    "dynamoose": "^3.0.0",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.0.0 || ^7.0.0"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -c .commitlintrc.json -E HUSKY_GIT_PARAMS"
    }
  }
}
