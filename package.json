{
  "name": "nestjs-dynamoose",
  "version": "0.6.0",
  "description": "Nest - modern, fast, powerful node.js web framework (@dynamoose)",
  "author": "Hardys",
  "repository": "https://github.com/hardyscc/nestjs-dynamoose.git",
  "license": "MIT",
  "scripts": {
    "lint": "eslint \"lib/**/*.ts\" --max-warnings 0",
    "format": "prettier \"lib/**/*.ts\" --write",
    "build": "rm -rf dist && tsc -p tsconfig.json",
    "precommit": "lint-staged",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "prepublish:next": "npm run build",
    "publish:next": "npm publish --access public --tag next",
    "prerelease": "npm run build",
    "release": "release-it"
  },
  "devDependencies": {
    "@commitlint/cli": "19.8.0",
    "@commitlint/config-angular": "19.8.0",
    "@nestjs/common": "11.0.20",
    "@nestjs/core": "11.0.20",
    "@types/node": "22.14.1",
    "dynamoose": "4.0.4",
    "eslint": "9.24.0",
    "eslint-config-prettier": "10.1.2",
    "eslint-plugin-prettier": "5.2.6",
    "globals": "16.0.0",
    "husky": "9.1.7",
    "lint-staged": "15.5.1",
    "prettier": "3.5.3",
    "reflect-metadata": "0.2.2",
    "release-it": "18.1.2",
    "rxjs": "7.8.2",
    "rxjs-compat": "6.6.7",
    "ts-node": "10.9.2",
    "typescript": "5.8.3",
    "typescript-eslint": "8.29.1"
  },
  "peerDependencies": {
    "@aws-sdk/client-dynamodb": "^3.0.0",
    "@nestjs/common": "^8.0.0 || ^9.0.0 || ^10.0.0 || ^11.0.0",
    "@nestjs/core": "^8.0.0 || ^9.0.0 || ^10.0.0 || ^11.0.0",
    "dynamoose": "^3.2.0 || ^4.0.0",
    "reflect-metadata": "^0.1.13 || ^0.2.0",
    "rxjs": "^6.0.0 || ^7.0.0"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -c .commitlintrc.json -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {}
}
