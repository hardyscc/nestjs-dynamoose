{
  "name": "nestjs-dynamoose",
  "version": "0.5.8",
  "description": "Nest - modern, fast, powerful node.js web framework (@dynamoose)",
  "author": "Hardys",
  "repository": "https://github.com/hardyscc/nestjs-dynamoose.git",
  "license": "MIT",
  "scripts": {
    "lint": "eslint \"lib/**/*.ts\" --max-warnings 0",
    "format": "prettier \"lib/**/*.ts\" --write",
    "build": "rm -rf dist && tsc -p tsconfig.json",
    "precommit": "lint-staged",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "prepublish:next": "npm run build",
    "publish:next": "npm publish --access public --tag next",
    "prerelease": "npm run build",
    "release": "release-it"
  },
  "devDependencies": {
    "@commitlint/cli": "18.6.0",
    "@commitlint/config-angular": "18.6.0",
    "@nestjs/common": "10.3.2",
    "@nestjs/core": "10.3.2",
    "@types/node": "20.11.17",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "dynamoose": "4.0.0",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "husky": "8.0.3",
    "lint-staged": "15.2.2",
    "prettier": "3.2.5",
    "reflect-metadata": "0.2.1",
    "release-it": "17.0.3",
    "rxjs": "7.8.1",
    "rxjs-compat": "6.6.7",
    "ts-node": "10.9.2",
    "typescript": "5.3.3"
  },
  "peerDependencies": {
    "@aws-sdk/client-dynamodb": "^3.0.0",
    "@nestjs/common": "^6.0.0 || ^7.0.0 || ^8.0.0 || ^9.0.0 || ^10.0.0",
    "@nestjs/core": "^6.0.0 || ^7.0.0 || ^8.0.0 || ^9.0.0 || ^10.0.0",
    "dynamoose": "^3.2.0 || ^4.0.0",
    "reflect-metadata": "^0.1.13 || ^0.2.0",
    "rxjs": "^6.0.0 || ^7.0.0"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -c .commitlintrc.json -E HUSKY_GIT_PARAMS"
    }
  }
}
